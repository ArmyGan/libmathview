<font-configuration>
  <font type="x" map="plain8"
	family="monospace" weight="normal" style="normal"
	x-foundry="b&amp;h" x-family="lucidatypewriter" x-weight="medium" x-slant="r" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="monospace" weight="bold" style="normal"
	x-foundry="b&amp;h" x-family="lucidatypewriter" x-weight="bold" x-slant="r" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="sans-serif" weight="normal" style="normal"
	x-foundry="b&amp;h" x-family="lucida" x-weight="medium" x-slant="r" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="sans-serif" weight="normal" style="italic"
	x-foundry="b&amp;h" x-family="lucida" x-weight="medium" x-slant="i" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="sans-serif" weight="bold" style="normal"
	x-foundry="b&amp;h" x-family="lucida" x-weight="medium" x-slant="r" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="sans-serif" weight="bold" style="italic"
	x-foundry="b&amp;h" x-family="lucida" x-weight="bold" x-slant="i" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="serif" weight="normal" style="normal"
	x-foundry="b&amp;h" x-family="lucidabright" x-weight="medium" x-slant="r" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="serif" weight="normal" style="italic"
	x-foundry="b&amp;h" x-family="lucidabright" x-weight="medium" x-slant="i" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="serif" weight="bold" style="normal"
	x-foundry="b&amp;h" x-family="lucidabright" x-weight="bold" x-slant="r" x-registry="iso8859" x-encoding="1"
	/>

  <font type="x" map="plain8"
	family="serif" weight="bold" style="italic"
	x-foundry="b&amp;h" x-family="lucidabright" x-weight="bold" x-slant="i" x-registry="iso8859" x-encoding="1"
	/>
</font-configuration>
