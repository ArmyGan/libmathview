<math-engine-configuration>
  <key name="version">@VERSION@</key>

  <section name="logger">
    <key name="verbosity">0</key>
  </section>

  <section name="dictionary">
    <key name="path">@prefix@/share/gtkmathview/dictionary.xml</key>
    <key name="path">@prefix@/share/gtkmathview/dictionary-local.xml</key>
    <key name="path">@prefix@/share/gtkmathview/dictionary-combining.xml</key>
  </section>

  <section name="default">
    <key name="font-size">12pt</key>
    <section name="color">
      <key name="foreground">#000000</key>
      <key name="background">#ffffff</key>
    </section>
    <section name="link-color">
      <key name="foreground">#32648b</key>
      <key name="background">#ffffff</key>
    </section>
    <section name="select-color">
      <key name="foreground">#000000</key>
      <key name="background">#ff6a6a</key>
    </section>
  </section>

